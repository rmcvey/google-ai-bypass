{
    "manifest_version": 3,
    "name": "Google Search Bypass AI",
    "description": "no-code extension restores Google Search to use Web results by default",
    "version": "1.0",
    "author": "Rob McVey <me@robmcvey.comn>",
    "permissions": [
      "declarativeNetRequest",
      "declarativeNetRequestWithHostAccess"
    ],
    "host_permissions": [
        "*://*/*"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "declarative_net_request": {
      "rule_resources": [
        {
          "id": "ruleset_1",
          "enabled": true,
          "path": "rules.json"
        }
      ]
    }
  }
  